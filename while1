//=-=-=-=-=-=-=-=-==--==-=--==--=-==--=
//Вывод на экран англиского алфавита и соответствующих ему кодов ASCII
//V 1.0
//=-=-==--=-=-=-=-===-=--=-=-==--=-=-==
#include<iostream>
using namespace std;
int main() {
	char ch = 'a';
	int count = 1;
	while (ch <= 'z') {
		cout << "#" << count << ": " << ch << "-" << static_cast<int>(ch) << " ";
		++ch;
		if (count % 3 == 0) {
			cout << "\n" << endl;
		}
		++count;
	}
	cout << "\n";
	return 0;
}
// Output:
/*
*/
//=-=-=-=-=-=-=-=-==--==-=--==--=-==--=
//
//End file
//=-=-==--=-=-=-=-===-=--=-=-==--=-=-==
