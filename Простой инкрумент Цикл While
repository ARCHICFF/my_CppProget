//-==-=-=-=-=-=-=-=-=-=--=-=-=-=-=-=-=-=-=-=-=-=
//Простой инкрумент Цикл While
//v1.0
//-=-==-=-=-=-=--===--==-=-=-=-=-=-=-=-=--=-=-==
#include<iostream>
using namespace std;

int main() {
	char en = 'a';
	while (en <= 'z') {
		++en;
		cout << en << "\n";
	}
	cout << " " << "\n";
	return 0;
}
/* Output

*/
